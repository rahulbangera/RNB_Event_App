{
	"compilerOptions": {
		"target": "es2022",
		"module": "ESNext",
		"moduleResolution": "node",
		"moduleDetection": "force",
		"esModuleInterop": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"outDir": "./dist",
		"types": ["node"]
	},
	"include": ["src", "prisma"],
	"exclude": ["node_modules", "dist"]
}
